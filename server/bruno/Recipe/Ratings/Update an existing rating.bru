meta {
  name: Update an existing rating
  type: http
  seq: 3
}

put {
  url: {{hostname}}/api/ratings/{{ratingId}}
  body: json
  auth: bearer
}

headers {
  Content-Type: application/json
}

auth:bearer {
  token: {{jwtToken}}
}

body:json {
  {
    "rating": 4,
    "comment": "Still great, but maybe could use a tiny bit more salt."
  }
}

settings {
  encodeUrl: true
}
